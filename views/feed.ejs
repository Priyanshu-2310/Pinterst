<% include ./partials/header.ejs %>
    <div class="w-full  min-h-screen bg-zinc-800 text-white">
        <div class="cards w-full flex flex-wrap gap-10 px-10 py-8">
            <% posts.forEach(function(elem){ %>
                <div class="w-32 card">
                    <div class=" w-32 h-40 bg-sky-200 rounded-lg overflow-hidden">
                        <img class="w-full h-full object-cover" src="/images/uploads/<%= elem.image %>" alt="">
                    </div>
                    <h3 class="mt-2 text-sm"><%= elem.title %></h3>
                    <!-- <h5 class="mt-2 text-xs "><%= elem.description %></h5> -->
                    <h5 class="text-xs">@<%=elem.user.username %></h5>
                </div>
             <% }) %>
           
        </div>
    </div>
<% include ./partials/footer.ejs %>